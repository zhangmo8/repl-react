{
  "name": "@zhangmo8/repl-react",
  "version": "0.0.2-beta.1",
  "type": "module",
  "files": [
    "dist"
  ],
  "main": "./dist/your-component-library.umd.js",
  "module": "./dist/your-component-library.es.js",
  "types": "./dist/index.d.ts",
  "publishConfig": {
    "access": "public"
  },
  "exports": {
    ".": {
      "import": "./dist/your-component-library.es.js",
      "require": "./dist/your-component-library.umd.js",
      "types": "./dist/index.d.ts"
    }
  },
  "scripts": {
    "dev": "vite",
    "build": "tsc -b && vite build",
    "build-preview": "vite build -c vite.preview.config.ts",
    "lint": "biome lint",
    "format": "biome format --write ./src",
    "preview": "vite preview",
    "prepublishOnly": "nr sync-docs && nr build",
    "release": "nr format && bumpp -r"
  },
  "dependencies": {
    "react": "^18.3.1",
    "react-dom": "^18.3.1"
  },
  "devDependencies": {
    "@biomejs/biome": "1.9.4",
    "@codemirror/autocomplete": "^6.18.3",
    "@codemirror/lang-javascript": "^6.2.2",
    "@codemirror/language": "^6.10.3",
    "@codemirror/state": "^6.4.1",
    "@lezer/highlight": "^1.2.1",
    "@swc/wasm-typescript-esm": "^1.10.1",
    "@swc/wasm-web": "^1.10.1",
    "@types/codemirror": "^5.60.15",
    "@types/node": "^22.9.1",
    "@types/react": "^18.3.12",
    "@types/react-dom": "^18.3.1",
    "@vitejs/plugin-react": "^4.3.3",
    "bumpp": "^9.9.1",
    "codemirror": "^6.0.1",
    "fflate": "^0.8.2",
    "globals": "^15.11.0",
    "typescript": "~5.6.2",
    "typescript-eslint": "^8.11.0",
    "vite": "^5.4.11",
    "vite-plugin-dts": "^4.3.0"
  }
}
